{
  "name": "francoisromain.github.io.postcss-grid-fluid",
  "version": "0.1.0",
  "description": "gh-pages for postcss-grid-fluid",
  "author": "Francois Romain <francoisromain@gmail.com>",
  "license": "MIT",
  "repository": "francoisromain/postcss-grid-fluid",
  "scripts": {
    "babel": "babel postcss-grid-fluid/src --out-dir postcss-grid-fluid/lib",
    "css-grid-fluid": "postcss --local-plugins -u ./postcss-grid-fluid src/grid-fluid.css -o dist/grid-fluid.css",
    "css-01": "postcss --local-plugins -u ./postcss-grid-fluid -i test/src/01.css -o test/dist/01.css",
    "css-02": "postcss --local-plugins -u ./postcss-grid-fluid -i test/src/02.css -o test/dist/02.css",
    "build": "npm run babel && npm run css",
    "test": "npm run babel && npm run css-grid-fluid && npm run css-01 && npm run css-02",
    "watch": "onchange 'index.html' 'src/**/*.*' 'postcss-grid-fluid/src/**/*.*' -- npm run build",
    "reload": "livereload",
    "start": "npm-run-all --parallel watch reload"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "livereload": "^0.4.1",
    "npm-run-all": "^2.0.0",
    "onchange": "^2.4.0",
    "postcss": "^5.0.21",
    "postcss-cli": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}